{
  "name": "jira-cli",
  "description": "A JIRA Command Line Interface",
  "version": "2.4.0",
  "author": "Dan Shumaker <danpshumaker@gmail.com> (https://danshumaker.github.io/)",
  "contributors": [],
  "scripts": {
    "test": "jest",
    "test:e2e": "bin/tests.sh && exit 1",
    "patch-release": "npm version patch && npm publish && git push --follow-tags",
    "dev": "ts-node src/entrypoint.ts",
    "start": "npm i -g && jira",
    "clean:dev": "rimraf -rf ./.jira-cli",
    "clean": "rimraf -rf ~/.jira-cli && npm uninstall jira-cli -g",
    "lint": "eslint . --ext .js,.ts --ignore-path .gitignore",
    "fix": "npm run lint -- --fix",
    "build": "tsc -p ."
  },
  "dependencies": {
    "alasql": "^0.6.5",
    "async": "^2.6.3",
    "cheerio": "^1.0.0-rc.5",
    "cli-table": "^0.3.4",
    "commander": "^7.0.0",
    "dargs": "^5.1.0",
    "fs": "0.0.2",
    "inquirer": "^7.3.3",
    "jira-connector": "^3.1.0",
    "moment": "^2.29.1",
    "nodemailer": "^6.4.17",
    "openurl": "^1.1.1",
    "qs": "^6.9.6",
    "strip-json-comments": "^2.0.1",
    "superagent": "~3.8.3",
    "underscore": "^1.12.0"
  },
  "engines": {
    "node": ">= 0.8"
  },
  "keywords": [
    "jira",
    "api",
    "client",
    "command",
    "cli",
    "tool",
    "command-line-junky"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/danshumaker/jira-cli.git"
  },
  "bin": {
    "jira": "bin/jira.js"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test"
    }
  },
  "devDependencies": {
    "@types/jest": "^25.2.3",
    "@types/async": "^3.2.5",
    "@types/cli-table": "^0.3.0",
    "@types/commander": "^2.12.2",
    "@types/inquirer": "^7.3.1",
    "@types/node": "^14.14.22",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "ava": "^3.15.0",
    "eslint": "^6.8.0",
    "husky": "^4.3.8",
    "jest": "^25.5.4",
    "rimraf": "^3.0.2",
    "testr": "0.0.5",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
